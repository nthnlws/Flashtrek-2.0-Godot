config_version=5

[application]

config/name="FlashTrek-2.0"
run/main_scene="res://scenes/3DMenu.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="Flashtrek"
config/features=PackedStringArray("4.2", "Forward Plus")
config/icon="res://assets/icons/icon_256.png"
config/windows_native_icon="res://assets/icons/icon_256.ico"

[autoload]

GameSettings="*res://scripts/gameSettings.gd"
SignalBus="*res://scripts/SignalBus.gd"
MiniMapGlobal="*res://scripts/MiniMapGlobal.gd"
DiscordManager="*res://scripts/DiscordManager.gd"
LevelManager="*res://scripts/levelManager.gd"
Utility="*res://utility.gd"

[debug]

gdscript/warnings/unused_parameter=0

[display]

window/size/viewport_width=960
window/size/viewport_height=540
window/size/resizable=false
window/size/window_width_override=960
window/size/window_height_override=540
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"

[editor]

movie_writer/movie_file="C:/Users/nthnl/Desktop/Code/Flashtrek-2.0-Godot/Exports/movie_recording.avi"

[editor_plugins]

enabled=PackedStringArray("res://addons/Todo_Manager/plugin.cfg", "res://addons/discord-rpc-gd/plugin.cfg", "res://addons/sprite_painter/plugin.cfg")

[import]

exclude_extensions=["svg"]

